#include<iostream>
#include<cstdlib>
using namespace std;

//in order to build a valid triangle the sum of two sides has to be bigger then the third side

bool triangle (int a, int b, int c)
{
  if(a>0 && b>0 && c>0 && (a+b)>c && (a+c)>b && (b+c)>a) return true;
  return false;
}

int main()
{
    int a, b, c;
   cin>>a>>b>>c;
   if(triangle(a, b, c))
     cout<<"You can build a triangle with these\n";
   else
     cout<<"You can't build a triangle with these, sorry\n";

    system("pause");
    return 0;
}
